<!DOCTYPE html>
<html lang="en">
<% include partials/header.ejs %>
<body id="page-top" class="inner_page">
<!-- Navigation-->
<% include partials/searchableNav.ejs %>
<!-- Masthead-->

<header class="inner_banner">
    <div class="container px-4 px-lg-5 d-flex align-items-center justify-content-center">
        <div class="d-flex justify-content-center">
            <div class="text-center">
                <h2 class="mx-auto my-0 text-uppercase text-white">Progress Tracking </h2>
            </div>
        </div>
    </div>
</header>
<body>
<h1>Track Progress</h1>
<!-- Display progress tracking information here -->
<ul>
    <% completedWorkouts.forEach(workout => { %>
        <li>Date Completed: <%= workout.dateCompleted %></li>
        <li>Duration: <%= workout.duration %> minutes</li>
        <li>Calories Burned: <%= workout.caloriesBurned %></li>
        <li>Weight Gained/Lost: <%= workout.weightGainedLost %></li>
        <!-- Display other metrics as needed -->
    <% }); %>
</ul>
</body>


